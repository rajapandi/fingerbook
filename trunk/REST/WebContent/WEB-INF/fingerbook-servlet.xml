<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  	<bean class="org.springframework.web.servlet.mvc.annotation.DefaultAnnotationHandlerMapping" /> 
  	<bean class="org.springframework.web.servlet.mvc.annotation.AnnotationMethodHandlerAdapter" /> 
  	
  	<bean id="FingerbookServices" class="fingerbook.service.FingerbookServices" />
  	<bean id="FingerbooksController" class="fingerbook.web.FingerbooksController">
  		<property name="FingerbookServices" ref="FingerbookServices" />
  	</bean>
  	
  	<bean name="/fingerbooks" class="fingerbook.web.FingerbooksController">
	</bean>
	
	<bean name="/*" class="fingerbook.web.ErrorController">
	</bean>

</beans>